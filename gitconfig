[push]
	default = current
[credential]
	helper = cache --timeout=360000
[alias]
	a = add
	b = branch
	c = commit
	cm = commit -m
	cam = commit -am
	co = checkout
	d = diff
	s = status
	b = branch
	r = remote
	l = log
	res = reset
[core]
	editor = vim
	excludesfile = ~/.gitignore
	fileMode = false
	trustctime = false
[filter "lfs"]
	required = true
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	clean = git-lfs clean -- %f

[oh-my-zsh]
	hide-dirty = 1
[diff]
	renameLimit = 100000
